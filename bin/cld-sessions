#!/bin/bash
HELP_DESC=$(cat << 'EOL'
Interactive utility to watch active classicdeploy user sessions in real time
EOL
)
HELP_EXAMPLES=$(cat << 'EOL'
cld-sessions
EOL
)
HELP_ONLY="CLI WEB"
source /var/cld/bin/include/cldfuncs
echo
echo Please choose one of active sessions below
echo
select FILE in $(ps ax | grep tee | grep -v grep | awk '{print $7}')
do
while read -r LINE; do
cat << EOL
$LINE
EOL
read -s < "$(tty 0>&2)"
done < $FILE
break
done
